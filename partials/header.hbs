<header>
	<a class="image-logo" href="/">
		<img src="{{asset 'images/logo.png'}}">
	</a>
	{{navigation}}
	<nav class="nav-social">
		{{#if @site.facebook}}
		<a href="{{facebook_url @site.facebook}}" target="_blank" rel="noopener" aria-label="Facebook">
			<span class="icon-facebook"></span>
		</a>
		{{/if}}
		{{#if @site.twitter}}
		<a href="{{twitter_url @site.twitter}}" target="_blank" rel="noopener" aria-label="Twitter">
			<span class="icon-twitter"></span>
		</a>
		{{/if}}
		<a href="https://www.linkedin.com/company/ieee-ucb/" target="_blank" rel="noopener" aria-label="LinkedIn">
			<span class="icon-linkedin"></span>
		</a>
		<a href="https://www.instagram.com/ieeeucb/" target="_blank" rel="noopener" aria-label="Instagram">
			<span class="icon-instagram"></span>
		</a>
	</nav>
</header>
